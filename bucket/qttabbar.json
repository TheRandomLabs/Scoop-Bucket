{
    "homepage": "http://qttabbar.wikidot.com/",
    "description": "A shell extension for the File Explorer that transforms it into a tabbed browser with many other customizable functions and features.",
    "license": "Unknown",
    "version": "1040",
    "url": [
        "http://qttabbar.wdfiles.com/local--files/qttabbar/UpdateQTTabBar1040.zip",
        "http://qttabbar.wdfiles.com/local--files/qttabbar/QTTabBar_1038.zip"
    ],
    "installer": {
        "file": "QTTabBar.exe", 
        "args": "/qi",
        "keep": "true"
    },
    "post_install": [
        "Start-Process -Wait \"$($dir)\\UpdateQTTabBar1040.exe\"",
        "Stop-Process -ProcessName explorer"
    ],
    "uninstaller": {
        "file": "QTTabBar.exe",
        "args": "/qu"
    }
}
