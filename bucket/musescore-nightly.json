{
    "version": "2019-11-08-1302-master-4cd9730",
    "description": "Music notation editor with an easy-to-use WYSIWYG interface.",
    "homepage": "https://musescore.org/",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://ftp.osuosl.org/pub/musescore-nightlies/windows/MuseScoreNightly-2019-11-08-1302-master-4cd9730-x86_64.7z",
            "hash": "645b90d1954d0f80aab3391f02a2bf0e755732921177099b1f202a37705dd0a7"
        }
    },
    "notes": "Register MuseScore file associations by running: \"$dir\\musescore-install-file-associations.reg\"",
    "extract_dir": "MuseScoreNightly",
    "post_install": [
        "$null, $bucket = find_manifest($app)",
        "$bucketdir = Find-BucketDirectory($bucket)",
        "(Get-Content \"$bucketdir\\..\\scripts\\musescore-install-file-associations.reg\").Replace('$musescore', \"$dir\\bin\\nightly.exe\".Replace('\\', '\\\\')) | Set-Content -Path \"$dir\\musescore-install-file-associations.reg\"",
        "Copy-Item \"$bucketdir\\..\\scripts\\musescore-uninstall-file-associations.reg\" -Destination \"$dir\\musescore-uninstall-file-associations.reg\""
    ],
    "bin": [
        [
            "bin\\nightly.exe",
            "MuseScore"
        ],
        [
            "bin\\nightly.exe",
            "mscore"
        ]
    ],
    "shortcuts": [
        [
            "bin\\nightly.exe",
            "MuseScore"
        ]
    ],
    "checkver": {
        "url": "https://ftp.osuosl.org/pub/musescore-nightlies/windows/",
        "regex": "MuseScoreNightly-([\\w-]+)-x86_64\\.7z"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://ftp.osuosl.org/pub/musescore-nightlies/windows/MuseScoreNightly-$version-x86_64.7z"
            }
        }
    }
}
