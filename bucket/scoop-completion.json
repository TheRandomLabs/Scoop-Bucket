{
    "version": "0.2.3",
    "description": "Tab completion for Scoop.",
    "homepage": "https://github.com/Moeologist/scoop-completion",
    "license": "MIT",
    "url": "https://github.com/Moeologist/scoop-completion/archive/v0.2.3.zip",
    "hash": "59fcda6e723b2c0bf7430668ccfb57086d82e9472733a01f9f7f9315768b5e5f",
    "notes": [
        "If you're getting errors, run `scoop config alias @{ }`.",
        "See here for more information: https://github.com/lukesampson/scoop/issues/3528",
        "The module is only automatically appended to the local $PROFILE."
    ],
    "extract_dir": "scoop-completion-0.2.3",
    "installer": {
        "script": [
            "Move-Item \"$dir\\src\\*\" -Destination $dir",
            "Remove-Item -Recurse \"$dir\\bucket\", \"$dir\\src\"",
            "Add-Content -ErrorAction Ignore $PROFILE \"Import-Module '$scoopdir\\modules\\scoop-completion'\" -Encoding Ascii"
        ]
    },
    "uninstaller": {
        "script": [
            "if (Test-Path $PROFILE) {",
            "    $content = Get-Content $PROFILE | Where-Object { $_ -notlike 'Import-Module*scoop-completion*' }",
            "    Set-Content $PROFILE $content -Encoding Ascii",
            "}"
        ]
    },
    "psmodule": {
        "name": "scoop-completion"
    },
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/Moeologist/scoop-completion/archive/v$version.zip",
        "extract_dir": "scoop-completion-$version"
    }
}
